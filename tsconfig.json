{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    ".yarn",
    ".pnp.cjs",
    ".pnp.loader.mjs"
  ]
}

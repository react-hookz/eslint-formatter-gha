{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "target": "ES5",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": [
      "ESNext"
    ],
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
